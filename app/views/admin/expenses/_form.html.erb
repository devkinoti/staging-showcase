<div>
	<%= form_for [:admin, @expense] do |f| %>
	<div>
		<%= f.label :account_type %>
		<%= f.text_field :account_type %>
	</div>
	<div>
		<%= f.label :description %>
		<%= f.text_area :description %>
	</div>
	<div>
		<%= f.label :amount %>
		<%= f.text_field :amount %>
	</div>
	<div>
		<%= f.submit %>
	</div>
	<%end%>
</div>